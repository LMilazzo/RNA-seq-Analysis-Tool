plotGeneCounts <- function(gene, metadata){
name <- (gene %>% select(gene_name))[1]
id <- (gene %>% select(gene_id))[1]
p <- (gene %>% select(padj))[1]
fold <- (gene %>% select(log2FoldChange))[1]
counts <- (gene[,5:ncol(gene)])
mean <- rowMeans(counts)
print(name)
print(id)
print(p)
print(fold)
print(counts)
print(mean)
}
